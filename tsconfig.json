{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "public/Data/Data.ts", "app/Context/AppointContext.tsx", "app/Components/Doctors/DoctorCart.tsx", "app/Components/Home.tsx", "app/appointment/page.tsx", "app/Components/Doctors/Doctors.jsx", "app/Components/Navigation/Nav.tsx"],
  "exclude": ["node_modules"]
}
